{
    "projectURL": "http://localhost:8080",
    "productURL": "./",
    "browserAutoOpen": false,
    "injectChanges": true,
    "styleSRC": "./src/scss/style.scss",
    "styleDestination": "./templates/static/css",
    "outputStyle": "compact",
    "errLogToConsole": true,
    "precision": 10,
    "jsCustomSRC": "./src/js/main.js",
    "jsCustomDestination": "./templates/static/js",
    "jsCustomFile": "main",
    "imgSRC": "./src/img/*",
    "imgDST": "./templates/static/img",
    "watchStyles": "./src/scss/*.scss",
    "watchJsCustom": "./src/js/*.js",
    "watchHTML": "./templates/**/*.html",
    "BROWSERS_LIST": [
      "last 2 version",
      "> 1%",
      "ie >= 11",
      "last 1 Android versions",
      "last 1 ChromeAndroid versions",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions",
      "last 2 iOS versions",
      "last 2 Edge versions",
      "last 2 Opera versions"
    ]
  }